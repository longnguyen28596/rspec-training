# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history
/config/application.yml
/data

config/settings.local.yml
config/settings/*.local.yml
config/environments/*.local.yml
/public/uploads
/public/assets

# google app engine
app.yaml
worker.yaml
cloud_sql_proxy
config/serviceaccounts/*
!keep
!config/serviceaccounts/sql-manager.sql.enc
!config/serviceaccounts/pubsub-manager.json.enc
